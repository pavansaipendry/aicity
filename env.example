# LLM Keys — fill in your actual keys
ANTHROPIC_API_KEY=your_anthropic_key_here
OPENAI_API_KEY=your_openai_key_here

# Groq (free cloud Llama inference — no download, no disk space, fastest inference)
# Get a free key at: https://console.groq.com → "Create API Key" (no credit card)
# Free tier: 14,400 requests/day — more than enough for any simulation run.
# Without this key, merchants and messengers fall back to GPT-4o automatically.
GROQ_API_KEY=your_groq_key_here
GROQ_MODEL=llama-3.3-70b-versatile

# Memory & Database
REDIS_URL=redis://localhost:6379
QDRANT_URL=http://localhost:6333
DATABASE_URL=postgresql://aicity:aicity@127.0.0.1:5433/aicity

# AIcity Config
AICITY_MAX_AGENTS=1000
AICITY_STARTING_TOKENS=1000
AICITY_DAILY_BURN_RATE=100
AICITY_MAX_TOKEN_SUPPLY=10000000
AICITY_TAX_RATE=0.10

# Security — generate long random strings for these
PAVAN_RED_BUTTON_KEY=generate_a_very_long_random_string_here
SECRET_KEY=another_very_long_random_string_here

# Environment
ENVIRONMENT=development
LOG_LEVEL=DEBUG
